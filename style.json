{
	"QCustomSlideMenu": [
		{
			"name": "leftMenuContainer",
			"defaultSize": [
				{
					"width": 55,
					"height": "parent"
				}
			],
			"collapsedSize": [
				{
					"width": 55,
					"height": "parent"
				}
			],
			"expandedSize": [
				{
					"width": 155,
					"height": "parent"
				}
			],
			"toggleButton": [
				{
					"buttonName": "menuBtn",
