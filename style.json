{
	"QMainWindow": [
		{
			"tittle": "DomoticHouse",
			"icon": "",
			"frameless": true,
			"transluscentBg": true,
			"sizeGrip": "size_grip",
			"navigation": [
				{
					"minimize": "minimizeBtn",
					"close": "closeBtn",
					"restore": [
						{
							"buttonName": "maximizeBtn",
							"normalIcon": ":/icons/icons/square.svg",
							"maximizedIcon": ":/icons/icons/copy.svg"
						}
					],
					"moveWindow": "headerContainer",
					"tittleBar": "headerContainer"
				}
			]
		}
	],
	"QCustomSlideMenu": [
		{
			"name": "leftMenuContainer",
			"defaultSize": [
				{
					"width": 55,
					"height": "parent"
				}
			],
			"collapsedSize": [
				{
					"width": 55,
					"height": "parent"
				}
			],
			"expandedSize": [
				{
					"width": 155,
					"height": "parent"
				}
			],
			"toggleButton": [
				{
					"buttonName": "menuBtn",
					"icons": [
						{
							"whenMenuIsCollapsed": ":/icons/icons/menu.svg",
							"whenMenuIsExpanded": ":/icons/icons/chevron-left.svg"
						}
					]
				}
			],
			"menuTransitionAnimation": [
				{
					"animationDuration": 250,
					"animationEasingCurve": "Linear"
				}
			]
		}
	],
	"QCustomQStackedWidget": [
		{
			"name": "mainPages",
			"navigation": [
				{
					"navigationButtons": [
						{
							"livingroomBtn": "livingroomPage",
							"officeBtn": "officePage",
							"bedroomBtn": "bedroomPage",
							"settingsBtn": "settingsPage"
						}
					]
				}
			]
		}
	],
	"QPushButtonGroup": [
		{
			"Buttons": [
				"livingroomBtn",
				"officeBtn",
				"bedroomBtn",
				"settingsBtn"
			],
			"Style": [
				{
					"Active": "background-color: #52a5e0;",
					"NotActive": "background-color: transparent;"
				}
			]
		}
	]
}